{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "lavender": "#f3f3f3",
    "mauve": "#cba6f7",
    "peach": "#fab387",
    "red": "#f38ba8",
    "green": "#a6e3a1",
    "yellow": "#f9e2af"
  },
  "transient_prompt": {
    "template": "‚ùØ ",
    "foreground": "#b4befe",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#f38ba8{{ end }}"
    ]
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "üçâ tontide1 "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:peach",
          "template": "Óàµ {{ .Venv }} ",
          "properties": {
            "display_default": true,
            "display_version": true
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:mauve",
          "template": "{{ .Path }} ",
          "properties": {
            "folder_separator_icon": " ‚ùØ ",
            "max_depth": 2,
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:green",
          "template": "Û∞ò¨ {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ‚Ä¢{{ .Working.String }}{{ end }}{{ if .Staging.Changed }} +{{ .Staging.String }}{{ end }}",
          "properties": {
            "branch_max_length": 15,
            "fetch_status": true
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:lavender",
          "template": "‚ùØ "
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
